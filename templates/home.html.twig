            disabledDates.push({
                from: (new Date('{{ booking.startDate|date('Y-m-d') }}')).setDate((new Date('{{ booking.startDate|date('Y-m-d') }}')).getDate() - 1),
                to: new Date('{{ booking.endDate|date('Y-m-d') }}')
            });
